{
	"prettier": {
		"allowed": true
	},
	"icon_theme": "Material Icon Theme",
	"edit_predictions": {
		"mode": "eager",
		"disabled_globs": [
			"**/node_modules/**",
			"**/dist/**",
			"**/.venv/**",
			"**/__pycache__/**"
		]
	},
	"features": {
		"edit_prediction_provider": "copilot"
	},
	"agent": {
		"default_model": {
			"provider": "copilot_chat",
			"model": "claude-sonnet-4"
		},
		"model_parameters": []
	},
	"ui_font_size": 16,
	"buffer_font_size": 16,
	"theme": {
		"mode": "system",
		"light": "Tokyo Night",
		"dark": "Tokyo Night"
	},
	"format_on_save": "on",
	"formatter": "prettier",
	"jsx_tag_auto_close": {
		"enabled": true
	},
	"soft_wrap": "editor_width"
}
